@model IEnumerable<Group_18_Final_Project.Models.Order>

    @{
        ViewData["Title"] = "View";
    }
    <br />
    <h2>Book Report</h2>

    <!--Display repository count-->
    <p>Displaying @ViewBag.SelectedOrders of @ViewBag.TotalOrders</p>


    <!--Repo list table-->
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.OrderID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.OrderDate)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.OrderID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.OrderDate)
                    </td>
                </tr>
            }
        </tbody>
    </table>
